App.Views.Settings=Backbone.View.extend({events:{"click .toggle-connect":"toggleConnect"},toggleConnect:function(n){var e=$(n.currentTarget);if(!e.hasClass("main-account")){var t=e.find("span:visible").data("url");t&&(e.hasClass("connected")?this.toggleConnectAjax(t).done(function(){analytics.track("account_"+e.find("span:visible").data("name")+"_unlinked"),e.toggleClass("connected not-connected")}):(analytics.track("account_"+e.find("span:visible").data("name")+"_linked"),window.location.href=t))}},toggleConnectAjax:function(n){return $.ajax({type:"POST",url:n})}});