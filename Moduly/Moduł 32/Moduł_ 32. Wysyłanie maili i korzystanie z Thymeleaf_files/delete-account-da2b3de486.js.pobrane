App.Views.DeleteAccount=Backbone.View.extend({events:{"click #acc-del-btn":"checkAccountDeleteCalculation"},initialize:function(e){this.$deleteModal=this.$el,this.$deleteModalCalculationText=this.$deleteModal.find("#calculation-text+p > span"),this.$deleteModalCalculationResult=this.$deleteModal.find("#calculation-result"),this.$calculationResult=0;var t=Math.round(5*Math.random())+1,l=Math.round(3*Math.random())+1,a=t+" x "+l+" =";this.$expectedCalculationResult=t*l,this.$deleteModalCalculationText.html(a)},checkAccountDeleteCalculation:function(){this.$expectedCalculationResult==this.$deleteModalCalculationResult.val()?this.accountDelete():alert(App.Translations("checkAccountDeleteCalculationMessage"))},accountDelete:function(){$.ajax({type:"POST",url:"/"+App.getLang()+"/account/delete"})}});